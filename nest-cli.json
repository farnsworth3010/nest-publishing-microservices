{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "compilerOptions": {
    "plugins": ["@nestjs/swagger/plugin"]
  },
  "projects": {
    "author": {
      "type": "application",
      "root": "apps/author",
      "entryFile": "main",
      "sourceRoot": "apps/author",
      "compilerOptions": {
        "tsConfigPath": "apps/author/tsconfig.app.json"
      }
    },
